    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.14",
    "vite": "^4.3.9"
  ,
  "typescript": "^5.1.6"
  }
