{
  "name": "vendor/affiliate-product-showcase",
  "description": "Affiliate Product Showcase WordPress plugin",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=7.4 <8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "AffiliateProductShowcase\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AffiliateProductShowcase\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --configuration phpunit.xml.dist"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
