{
  "name": "affiliate-product-showcase/plugin",
  "description": "Affiliate Product Showcase - plugin development dependencies",
  "type": "project",
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "brain/monkey": "^2.8",
    "mikey179/vfsstream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Tests\\": "tests/",
      "AffiliateProductShowcase\\": "wp-content/plugins/affiliate-product-showcase/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit --configuration phpunit.xml"
  }
}
